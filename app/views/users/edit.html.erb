<div id ="session_wrap" class="full">
    <div id="session_new">
  <%= form_for(@user) do |f| %>
    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true %>
      </div>

    <div class="field">
      <%= f.label :password %><br />
      <%= f.password_field :password, autocomplete: "off" %>
    </div> 
    <div class="actions">
      <%= f.submit "keio.jpにアクセス" %>
    </div>
  <% end %>
  </div>
</div>
